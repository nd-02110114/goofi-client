{
  "name": "goofi-client",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/expo": "30.0.2",
    "@types/expo__vector-icons": "^6.2.3",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.14",
    "@types/react-navigation": "^2.13.9",
    "@types/react-test-renderer": "^16.0.3",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^5.12.0",
    "ex-react-native-i18n-ts": "^0.0.6",
    "exp": "^57.2.1",
    "jest-expo": "30.0.0",
    "prettier": "^1.13.7",
    "react-native-scripts": "latest",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.3.1",
    "ts-jest": "^22.4.6",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "21.0.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry-web.js",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn",
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "jest",
    "tsc": "node_modules/.bin/tsc",
    "lint": "node_modules/.bin/tslint 'src/**/*.ts{,x}'",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "i18n-ts": "ex-react-native-i18n-ts",
    "i18n-ts:watch": "ex-react-native-i18n-ts -w"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/test/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|ex-react-native-i18n|expo(nent)?|@expo(nent)?/.*|react-navigation))"
    ]
  },
  "dependencies": {
    "apollo-boost": "^0.1.23",
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link-context": "^1.0.12",
    "apollo-link-http": "^1.5.9",
    "expo": "^30.0.0",
    "ex-react-native-i18n": "0.0.4",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "lodash": "^4.17.10",
    "react": "16.3.1",
    "react-apollo": "^2.3.3",
    "react-native": "~0.55.4",
    "react-native-modal": "^6.5.0",
    "react-navigation": "^2.18.0",
    "styled-components": "^3.3.3"
  },
  "ex-react-native-i18n-ts": {
    "model": "./src/locale/languages/ja.json",
    "outputDir": "./types/ex-react-native-i18n"
  }
}
