    "expo-web": "^0.0.12",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "react": "16.3.1",
    "react-apollo": "^2.1.8",
    "react-dom": "16.0.0",
    "react-native": "~0.55.2",
    "react-native-web": "^0.4.0",
    "react-navigation": "^2.6.0",
    "styled-components": "^3.3.3",
    "webpack": "^3.11.0",
    "webpack-dev-server": "2.9.4"
