{
  "name": "goofi-client",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "test": "jest",
    "e2e": "detox test --configuration ios.sim",
    "tsc": "tsc",
    "fmt": "prettier --write \"{src,test,assets}/**/*.{ts,tsx}\"",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "i18n-ts": "ex-react-native-i18n-ts",
    "i18n-ts:watch": "ex-react-native-i18n-ts -w"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/react": "16.8.23",
    "@types/react-native": "~0.57.65",
    "@types/react-navigation": "^3.0.8",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-preset-expo": "^7.0.0",
    "detox": "~12.3.0",
    "detox-expo-helpers": "^0.6.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "ex-react-native-i18n-ts": "^0.0.6",
    "expo-detox-hook": "^1.0.10",
    "jest": "^24.9.0",
    "jest-expo": "^35.0.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "ex-react-native-i18n": "0.0.6",
    "expo": "^35.0.0",
    "expo-font": "^7.0.0",
    "graphql": "^14.5.7",
    "graphql-tag": "^2.10.1",
    "react": "16.8.3",
    "react-apollo": "^3.1.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-modal": "^11.3.1",
    "react-native-reanimated": "~1.2.0",
    "react-native-webview": "7.0.5",
    "react-navigation": "^4.0.7",
    "react-navigation-stack": "^1.8.1",
    "react-navigation-tabs": "^2.5.5",
    "styled-components": "^4.3.2"
  },
  "ex-react-native-i18n-ts": {
    "model": "./src/locale/languages/ja.json",
    "outputDir": "./types/ex-react-native-i18n"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "test/e2e/config.json",
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent-2.11.1.app",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  }
}
